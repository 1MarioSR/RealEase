@using RealEase.Domain.Entities
@model Contract

@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

<div>
    <h4>Contract</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            ClientId
        </dt>
        <dd class="col-sm-10">
            @Model.ClientId
        </dd>
        <dt class="col-sm-2">
            AgentId
        </dt>
        <dd class="col-sm-10">
            @Model.AgentId
        </dd>
        <dt class="col-sm-2">
            PropertyId
        </dt>
        <dd class="col-sm-10">
            @Model.PropertyId
        </dd>
        <dt class="col-sm-2">
            StartDate
        </dt>
        <dd class="col-sm-10">
            @Model.StartDate
        </dd>
        <dt class="col-sm-2">
            EndDate
        </dt>
        <dd class="col-sm-10">
            @Model.EndDate
        </dd>
        <dt class="col-sm-2">
            MonthlyAmount
        </dt>
        <dd class="col-sm-10">
            @Model.MonthlyAmount
        </dd>
        <dt class="col-sm-2">
            Status
        </dt>
        <dd class="col-sm-10">
            @Model.Status
        </dd>
        
    </dl>
</div>
<div>
    <a asp-action="Edit" class="btn btn-secondary" asp-route-id="@Model?.Id">Edit</a> |
    <a asp-action="Index" class="btn btn-secondary">Back to List</a>
</div>
